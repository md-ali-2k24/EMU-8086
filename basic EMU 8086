=======> Find the Lergest number in 3number 
.model small
.stack 100h
.data 

var1 db "Input three numbers  : $"
var2 db 10,13,"The largest number is : $"

.code
main proc
    mov ax,@data
    mov ds,ax
    
    mov ah,9
    lea dx,var1
    int 21h
    
    mov ah,1
    int 21h
    mov bl,al
    int 21h
    mov bh,al
    int 21h
    mov cl,al

    cmp bl,bh
    jge a
    jmp b 
    
    a:
    cmp bl,cl
    jge e
    jmp d
    
    b:
    cmp bh,cl
    jge c
    jmp d
    
    
    c:
    mov ah,9
    lea dx,var2
    int 21h
    mov ah,2
    mov dl,bh        ;b
    int 21h
    jmp exit 
    
    d:
    mov ah,9
    lea dx,var2
    int 21h
    mov ah,2
    mov dl,cl        ;c
    int 21h
    jmp exit
    
    e:
    mov ah,9
    lea dx,var2
    int 21h
    mov ah,2
    mov dl,bl        ;a
    int 21h
    
    exit:
    mov ah,4ch
    int 21h
    main endp
end main

======> Constent concept.
.model small
.stack 100h
.data 

var1 db "Input three numbers  : $"
const equ 10,13,"5"
var2 equ 10,13,"Sihuuu : ",1,"$"
var db var2

.code
main proc
    mov ax,@data
    mov ds,ax
    
    mov ah,9
    lea dx,var1
    int 21h
    
    mov ah,1
    int 21h
    mov const,al
    
    mov ah,2
    mov dl,const
    int 21h
    
    mov ah,9
    lea dx,var
    int 21h
  
    exit:
    mov ah,4ch
    int 21h
    main endp
end main


