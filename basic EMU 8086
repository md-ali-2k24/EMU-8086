==================================
.model small 
.stack 100h
.data 

var db 'Enter a number for loop: $'
num db ?

.code
main proc
    mov ax,@data 
    mov ds,ax
    
    mov ah,9
    lea dx,var
    int 21h
    
    mov ah,1
    int 21h
    mov cl,al
    sub cl,48
    mov ch,0
    
    
    int cx,26
    mov bl,'A'
    
    level:
    mov ah,2
    mov dl,bl
    int 21h
    inc bl
    loop level
    
    edit:
    mov ah,4ch
    int 21h
    main endp
end main

