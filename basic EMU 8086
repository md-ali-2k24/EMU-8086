    int 21h
    mov bh,al
    
    mov ah,9
    lea dx,var3
    int 21h
    
    mov ah,1
    int 21h
    mov cl,al
    
    mov ah,9
    lea dx,var4
    int 21h
    add bl,bh
    sub bl,48       ;bl = bl-48
    add bl,cl
    sub bl,48       ;bl = bl-48
    mov ah,2
    mov dl,bl
    int 21h
    
    exit:
    mov ah,4ch
    int 21h
    main endp
end main

=================> Sum of 4 single number 
.model small
.stack 100h
.data

var1 db "Enter the first Number : $"
var2 db 10,13,"Enter the second Number : $" 
var3 db 10,13,"Enter the third Number : $"
var4 db 10,13,"Ender the fourth Numer : $"
var5 db 10,13,"Rsult of Sum : $"

.code

main proc
    mov ax,@data
    mov ds,ax
    
    mov ah,9
    lea dx,var1
    int 21h
    
    mov ah,1
    int 21h
    mov bl,al
    
    mov ah,9
    lea dx,var2
    int 21h
    
    mov ah,1
    int 21h
    mov bh,al
    
    mov ah,9
    lea dx,var3
    int 21h
    
    mov ah,1
    int 21h
    mov cl,al
    
    mov ah,9
    lea dx,var4
    int 21h
    
    mov ah,1
    int 21h
    mov ch,al
    
    mov ah,9
    lea dx,var5
    int 21h
    add bl,bh
    sub bl,48       ;bl = bl-48
    add bl,cl
    sub bl,48       ;bl = bl-48
    add bl,ch
    sub bl,48       ;bl = bl-48
    mov ah,2
    mov dl,bl
    int 21h
    
    exit:
    mov ah,4ch
    int 21h
    main endp
end main

=====>Loswer case to Upper case
.model
.stack 100h
.data 

var1 db "Lower form : $"
var2 db 10,13,"Upper form : $"

.code
main proc
    mov ax,@data 
    mov ds,ax
    
    mov ah,9
    lea dx,var1
    int 21h
    
    mov ah,1
    int 21h
    mov bl,al
    
    mov ah,9
    lea dx,var2
    int 21h
    
    mov ah,2
    sub bl,32
    mov dl,bl
    int 21h
    
    exit:
    mov ah,4ch
    main endp
end main
    
========> Upper case to Lower case
.model small
.stack 100h
